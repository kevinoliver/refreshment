{
  "name": "refreshment",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Automatically refresh tabs",
  "action": {
    "default_title": "Click to refresh this tab every 5 minutes."
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "activeTab", 
    "scripting",
    "alarms"
  ]
}
